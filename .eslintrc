{
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 2018
    },
    "env": {
        "node": true,
        "es6": true
    },
    "rules": {
        "no-cond-assign": [
            2,
            "always"
        ],
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-extra-semi": 2,
        "no-func-assign": 2,
        "no-invalid-regexp": 2,
        "no-obj-calls": 2,
        "use-isnan": 2,
        "valid-typeof": 2,
        "curly": [
            1,
            "multi-line"
        ],
        "dot-location": [
            1,
            "property"
        ],
        "eqeqeq": 2,
        "no-caller": 2,
        "no-else-return": 1,
        "no-extra-bind": 1,
        "no-fallthrough": 2,
        "no-floating-decimal": 2,
        "no-implicit-coercion": [
            1,
            {
                "boolean": true,
                "number": true,
                "string": true
            }
        ],
        "no-loop-func": 1,
        "no-multi-spaces": [
            1,
            {
                "ignoreEOLComments": true
            }
        ],
        "no-new-wrappers": 1,
        "no-octal-escape": 2,
        "no-octal": 2,
        "no-redeclare": [
            2,
            {
                "builtinGlobals": false
            }
        ],
        "no-self-compare": 2,
        "no-sequences": 1,
        "no-throw-literal": 1,
        "no-unused-expressions": 1,
        "no-useless-call": 1,
        "no-with": 1,
        "radix": 1,
        "no-delete-var": 2,
        "no-shadow-restricted-names": 1,
        "no-shadow": 1,
        "no-undef": 2,
        "no-unused-vars": 1,
        "no-use-before-define": [
            2,
            "nofunc"
        ],
        "callback-return": 1,
        "handle-callback-err": 1,
        "no-path-concat": 1,
        "brace-style": [
            1,
            "1tbs",
            {}
        ],
        "comma-spacing": [
            1,
            {
                "after": true
            }
        ],
        "comma-style": [
            1,
            "last"
        ],
        "eol-last": 1,
        "indent": [
            1,
            4
        ],
        "key-spacing": [
            1,
            {
                "beforeColon": false,
                "afterColon": true
            }
        ],
        "linebreak-style": [
            1,
            "unix"
        ],
        "new-cap": 1,
        "new-parens": 1,
        "no-multiple-empty-lines": [
            1,
            {
                "max": 2
            }
        ],
        "no-new-object": 1,
        "no-trailing-spaces": 1,
        "object-curly-spacing": [
            1,
            "always",
            {}
        ],
        "one-var": [
            1,
            "never"
        ],
        "operator-linebreak": [
            1,
            "after"
        ],
        "quotes": [
            1,
            "single"
        ],
        "semi": [
            2,
            "always"
        ],
        "keyword-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "space-before-blocks": [
            1,
            "always"
        ],
        "arrow-parens": [
            1,
            "always"
        ],
        "arrow-spacing": [
            1,
            {
                "before": true,
                "after": true
            }
        ],
        "constructor-super": 2,
        "no-const-assign": 2,
        "no-this-before-super": 2,
        "no-var": 1,
        "class-methods-use-this": 1,
        "prefer-promise-reject-errors": 1
    }
}